@model Quantity
@{
    ViewData["title"] = "Add Quantites";
}
<h3>Add Quantites</h3>
<div class="col-md-9">
    <form asp-action="StoreProductQuantity" method="post" asp-controller="Product" enctype="multipart/form-data">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" name="ProductId" , value="@ViewBag.ProductId"/>
        <div class="form-group">
            <label>Quantity</label>
            <input min="1" max="2000" asp-for="Quantities" class="form-control">
            <span asp-validation-for="Quantities" class="text-danger" />
        </div>

        <div class="form-group">
            <button class="btn btn-primary">Submit Create Category</button>
        </div>
    </form>
    <a asp-action="Product">Back to Product List</a>
</div>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}